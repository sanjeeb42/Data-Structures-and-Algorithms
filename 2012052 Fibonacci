
#include<stdio.h>
#define max 5000

int arr1[max],arr2[max],result[max];

void fun(void);

int main()
{
	int num,i,j,tag=0;
	// clrscr();
	for(i=0;i<max;i++)
		arr1[i]=arr2[i]=result[i]=0;

	arr2[max-1]=1;

	printf("ENTER THE TERM : ");
	scanf("%d",&num);

	for(i=0;i<num;i++)
	{
		fun();

		if(i==num-3)
			break;

		for(j=max-1;j>=0;j--)
			arr1[j]=arr2[j];

		for(j=max-1;j>=0;j--)
			arr2[j]=result[j];

	}

	for(i=0;i<max;i++)
	{
		if(tag||result[i])
		{
			tag=1;
			printf("%d",result[i]);
		}
	}


	getch();
	return 1;
}

void fun(void)
{
	int i,temp;
	for(i=0;i<max;i++)
		result[i]=arr1[i]+arr2[i];

	for(i=max-1;i>0;i--)
	{
		if(result[i]>9)
		{
			temp=result[i];
			result[i]%=10;
			result[i-1]+=(temp/10);
		}
	}
}
